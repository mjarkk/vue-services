{
    "extends": ["eslint:recommended", "plugin:vue/vue3-recommended"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": ["vue"],
    "globals": {},
    "rules": {
        "indent": ["warn", 4, {"SwitchCase": 1}],
        "semi": [2, "always"],
        "space-before-function-paren": "off",
        "comma-dangle": "off",
        "getter-return": "error",
        "no-console": "off",
        "no-prototype-builtins": "error",
        "no-unused-vars": ["error", {"argsIgnorePattern": "_", "args": "after-used"}]
    },
    "env": {
        "browser": true,
        "mocha": true,
        "es2021": true
    }
}
